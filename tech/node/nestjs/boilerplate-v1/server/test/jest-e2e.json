{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "..",
  "globalSetup": "<rootDir>/test/setup-e2e-tests.ts",
  "setupFiles": [
    "<rootDir>/src/internals/load-environment-variables.ts"
  ],
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  }
}
